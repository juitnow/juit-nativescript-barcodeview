{
  "name": "@juit/nativescript-barcodeview",
  "version": "1.1.1",
  "description": "Embedded barcode scanning view for NativeScript 7",
  "main": "dist/barcodeview",
  "types": "dist/index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "7.0.0",
      "ios": "7.0.0"
    }
